{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx sst bind next dev --stage dev",
    "build": "npx sst bind next build",
    "start": "next start",
    "lint": "next lint",
    "deploy:prod": "npx sst deploy --stage prod",
    "format": "prettier --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "format:fix": "prettier --write --ignore-path .gitignore --ignore-path .prettierignore .",
    "cleanAndBuild_COMMENT": "Sometimes the IDE just gets confused, cleaning and rebuilding helps in a lot of cases",
    "cleanAndBuild": "npm run clean && npm install && npm run nextBuild",
    "clean": "npm run clean:next && npm run clean:modules",
    "clean:next": "rimraf .next/",
    "clean:modules": "rimraf node_modules/",
    "nextBuild": "next build",
    "updateBrowserslist": "npx browserslist@latest --update-db"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.470.0",
