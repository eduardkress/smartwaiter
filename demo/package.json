    "@nextui-org/react": "^2.2.9",
    "@preact/signals": "^1.2.2",
    "aws-amplify": "^6.0.6",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.5",
    "next": "14.0.0",
    "next-auth": "^4.24.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "react-qr-code": "^2.0.12",
    "react-to-print": "^2.14.15",
